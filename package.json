{
  "name": "users-service",
  "version": "0.0.1",
  "private": true,
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "scripts": {
    "build": "npm-run-all clean transpile",
    "server": "node ./dist/server/bin/www",
    "dev": "npm-run-all build server",
    "transpile": "babel ./server --out-dir dist/server",
    "clean": "rimraf dist/se",
    "watch:dev": "nodemon"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.2",
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "mongoose": "^5.10.0",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^3.0.0"
  }
}
